mixin project-mixin(data)
		div.container-fluid
			div.row
				div.col-md-8.col-md-offset-2.proj-container
					div(class="row proj-data")
						div.project-label Project Name :  
						div.project-name #{data.Name}
					div(class="row proj-data")
						div.project-label Creator(s) : 
						div.project-name #{data.Creator}

